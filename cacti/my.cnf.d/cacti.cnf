[mysqld]
default_time_zone=Europe/Paris

[server]
innodb_flush_log_at_trx_commit = 2      # 1 > 2
innodb_additional_mem_pool_size = 80M   # 8M > 80M
innodb_buffer_pool_size = 444M          # 128M > 444M
innodb_large_prefix = 1                 # OFF > 1
innodb_file_format = Barracuda          # Antelope > Barracuda
innodb_file_per_table = ON              # OFF > ON
join_buffer_size = 57M                  # 0M > 57M
tmp_table_size = 28M                    # 16M > 28M
max_heap_table_size = 28M               # 16M > 28M
max_allowed_packet = 16777216           # 1048576 > 16777216
#character_set_client = utf8mb4         # latin1 > utf8mb4
#collation_server = utf8mb4_unicode_ci  # latin1_swedish_ci > utf8mb4_unicode_ci
